<?xml version="1.0" encoding="UTF-8"?>
<properties>
	<!--  
		是否使用数字认证中心服务
	-->
	<property name="ca.login.server" value="false" />
	
	<!-- 
	########  
	########Cookie名称配置 开始
	########
	-->
	<!-- 客户端IP地址 -->
	<property name="CLIENT_IP" value="KOAL_CLIENT_IP" />
	
	<!-- 证书有效期的起始时间 -->
	<property name="NOT_BEFORE" value="KOAL_NOT_BEFORE" />
	
	<!-- 证书有效期的中止时间 -->
	<property name="NOT_AFTER" value="KOAL_NOT_AFTER" />
	
	<!-- 证书序列号，简称SN，对于同一CA颁发的证书，这是一个唯一值（不同CA的证书序列号可能重复） -->
	<property name="CERT_SN" value="KOAL_CERT_SN" />
	
	<!-- 可识别名，简称DN，全称为DistinguishedName，可以作为证书的唯一标识 -->
	<property name="CERT_DN" value="KOAL_CERT_DN" />
	
	<!-- 通用名项，简称CN，对应X509规范中的id-at-commonName，如果是个人证书，则一般为人名，如“张三” -->
	<property name="CERT_CN" value="KOAL_CERT_CN" />
	
	<!-- 国家，简称C，对应X509规范中的id-at-countryName，例如“中国” -->
	<property name="CERT_C" value="KOAL_CERT_C" />
	
	<!-- 地点，简称L，对应X509规范中的id-at-localityName，一般为城市名，例如“上海” -->
	<property name="CERT_L" value="KOAL_CERT_L" />
	
	<!-- 省份，简称ST，对应X509规范中的id-at-stateOrProvinceName，一般为省份名称，例如“浙江” -->
	<property name="CERT_ST" value="KOAL_CERT_ST" />
	
	<!-- 组织，简称O，对应X509规范中的id-at-organizationName，一般为公司或团体名，如“格尔软件” -->
	<property name="CERT_O" value="KOAL_CERT_O" />
	
	<!-- 部门，简称OU，对应X509规范中的id-at-organizationalUnitName，一般为部门名，如“市场部” -->
	<property name="CERT_OU" value="KOAL_CERT_OU" />
	
	<!-- 电子邮件，对应X509规范中的pkcs9-emailAddress，一般为用户的电子邮件，如abc@koal.com -->
	<property name="CERT_E" value="KOAL_CERT_E" />
	
	<!-- 简称GN，对应X509规范中的id-at-givenName -->
	<property name="CERT_GN" value="KOAL_CERT_GN" />
	
	<!-- 简称T，对应X509规范中的id-at-title（较少使用） -->
	<property name="CERT_T" value="KOAL_CERT_T" />
	
	<!-- 简称S，对应X509规范中的id-at-surname（较少使用） -->
	<property name="CERT_S" value="KOAL_CERT_S" />
	
	<!-- 证书别名，某些CA颁发的证书可能有这一项 -->
	<property name="CERT_ALIAS" value="KOAL_CERT_ALIAS" />
	
	<!-- 电话号码，某些CA颁发的证书可能有这一项 -->
	<property name="CERT_PHONE" value="KOAL_CERT_PHONE" />
	
	<!-- 证书颁发者的CN，也即颁发用户证书的上级CA证书的id-at-commonName项  -->
	<property name="CERT_ISSUER_CN" value="KOAL_CERT_ISSUER_CN" />
	
	<!-- 证书颁发者的O，也即颁发用户证书的上级CA证书的id-at-organizationName项  -->
	<property name="CERT_ISSUER_O" value="KOAL_CERT_ISSUER_O" />
	
	<!-- 客户端证书，PEM格式，这是大约1k左右的数据，如果不是特殊情况不建议绑定到cookie，否则会给WEB服务器带来比较大的负担  -->
	<property name="CERT" value="KOAL_CERT" />
	<!-- 
	########  
	########cookies属性名配置 结束
	########
	-->

</properties>